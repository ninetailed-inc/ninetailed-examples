{
  "name": "@ninetailed/marketing-contentful-next",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "nextlint": "next lint",
    "nextlint:fix": "next lint --fix",
    "format": "prettier --write .",
    "contentful-typescript-codegen": "PATH_TO_ENV_FILE=.env.local contentful-typescript-codegen --output types/contentful.d.ts",
    "setup": "PATH_TO_ENV_FILE=.env.local node ./contentful/setup.js",
    "export": "PATH_TO_ENV_FILE=.env.local node ./contentful/export.js",
    "build-and-setup": "PATH_TO_ENV_FILE=.env node ./contentful/setup.js && next build",
    "delete-entries": "PATH_TO_ENV_FILE=.env.local CONTENT_TYPE=testType LIMIT=1000 node ./contentful/deleteEntriesOfContentType.js"
  },
  "dependencies": {
    "@aaronhayes/react-use-hubspot-form": "^2.1.1",
    "@contentful/rich-text-react-renderer": "15.12.1",
    "@contentful/rich-text-types": "15.14.1",
    "@heroicons/react": "1.0.6",
    "@ninetailed/experience.js-next": "^3.3.0-beta.0",
    "@ninetailed/experience.js-plugin-google-analytics": "^3.3.0-beta.0",
    "@ninetailed/experience.js-plugin-google-tagmanager": "^3.3.0-beta.0",
    "@ninetailed/experience.js-plugin-preview": "^3.3.0-beta.0",
    "@ninetailed/experience.js-utils-contentful": "^3.3.0-beta.0",
    "classnames": "2.3.1",
    "contentful": "9.2.15",
    "contentful-export": "7.18.16",
    "contentful-import": "8.3.0",
    "contentful-management": "10.15.1",
    "contentful-typescript-codegen": "3.2.3",
    "dotenv": "16.0.1",
    "final-form": "4.20.7",
    "lodash": "4.17.21",
    "next": "12.1.6",
    "next-seo": "5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@types/lodash": "4.14.181",
    "@types/node": "18.0.0",
    "@types/react": "17.0.43",
    "@types/yup": "0.29.14",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "autoprefixer": "10.4.4",
    "eslint": "^8.28.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.23",
    "typescript": "4.9.3"
  }
}
